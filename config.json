  "logger": {
    "INFO": {
      "format": ["DATE", "TYPE", "MSG"],
      "color": {
        "DATE": "bold",
        "TYPE": "blue.bold.bgBlack",
        "MSG": "bold"
      },
      "type": "INFO"
    },
    "ERROR": {
      "format": ["DATE", "TYPE", "MSG"],
      "color": {
        "DATE": "bold",
        "TYPE": "red.bold.bgBlack",
        "MSG": "bold"
